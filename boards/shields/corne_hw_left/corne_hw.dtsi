#include <dt-bindings/zmk/matrix_transform.h>

/ {
    chosen {
        zmk,kscan = &kscan0;
        zmk,physical_layout = &default_layout;
        zephyr,display = &oled0;
    };

    default_transform: keymap_transform_0 {
        compatible = "zmk,matrix-transform";
        columns = <12>;
        rows = <4>;
        map = <
RC(0,0) RC(0,1) RC(0,2) RC(0,3) RC(0,4) RC(0,5)  RC(0,6) RC(0,7) RC(0,8) RC(0,9) RC(0,10) RC(0,11)
RC(1,0) RC(1,1) RC(1,2) RC(1,3) RC(1,4) RC(1,5)  RC(1,6) RC(1,7) RC(1,8) RC(1,9) RC(1,10) RC(1,11)
RC(2,0) RC(2,1) RC(2,2) RC(2,3) RC(2,4) RC(2,5)  RC(2,6) RC(2,7) RC(2,8) RC(2,9) RC(2,10) RC(2,11)
                        RC(3,0) RC(3,1) RC(3,2)  RC(3,3) RC(3,4) RC(3,5)
        >;
    };

    key_attrs: key-attributes {
        compatible = "zmk,key-attributes";
        #key-cells = <7>;
    };

    default_layout: corne_layout {
        compatible = "zmk,physical-layout";
        display-name = "Corne";
        transform = <&default_transform>;
        
        keys
            = <&key_attrs 100 100    0    0 0 0 0>
            , <&key_attrs 100 100  100    0 0 0 0>
            , <&key_attrs 100 100  200    0 0 0 0>
            , <&key_attrs 100 100  300    0 0 0 0>
            , <&key_attrs 100 100  400    0 0 0 0>
            , <&key_attrs 100 100  500    0 0 0 0>
            , <&key_attrs 100 100  700    0 0 0 0>
            , <&key_attrs 100 100  800    0 0 0 0>
            , <&key_attrs 100 100  900    0 0 0 0>
            , <&key_attrs 100 100 1000    0 0 0 0>
            , <&key_attrs 100 100 1100    0 0 0 0>
            , <&key_attrs 100 100 1200    0 0 0 0>
            , <&key_attrs 100 100    0  100 0 0 0>
            , <&key_attrs 100 100  100  100 0 0 0>
            , <&key_attrs 100 100  200  100 0 0 0>
            , <&key_attrs 100 100  300  100 0 0 0>
            , <&key_attrs 100 100  400  100 0 0 0>
            , <&key_attrs 100 100  500  100 0 0 0>
            , <&key_attrs 100 100  700  100 0 0 0>
            , <&key_attrs 100 100  800  100 0 0 0>
            , <&key_attrs 100 100  900  100 0 0 0>
            , <&key_attrs 100 100 1000  100 0 0 0>
            , <&key_attrs 100 100 1100  100 0 0 0>
            , <&key_attrs 100 100 1200  100 0 0 0>
            , <&key_attrs 100 100    0  200 0 0 0>
            , <&key_attrs 100 100  100  200 0 0 0>
            , <&key_attrs 100 100  200  200 0 0 0>
            , <&key_attrs 100 100  300  200 0 0 0>
            , <&key_attrs 100 100  400  200 0 0 0>
            , <&key_attrs 100 100  500  200 0 0 0>
            , <&key_attrs 100 100  700  200 0 0 0>
            , <&key_attrs 100 100  800  200 0 0 0>
            , <&key_attrs 100 100  900  200 0 0 0>
            , <&key_attrs 100 100 1000  200 0 0 0>
            , <&key_attrs 100 100 1100  200 0 0 0>
            , <&key_attrs 100 100 1200  200 0 0 0>
            , <&key_attrs 100 100  300  300 0 0 0>
            , <&key_attrs 100 100  400  300 0 0 0>
            , <&key_attrs 100 100  500  300 0 0 0>
            , <&key_attrs 100 100  700  300 0 0 0>
            , <&key_attrs 100 100  800  300 0 0 0>
            , <&key_attrs 100 100  900  300 0 0 0>
            ;
    };
};
